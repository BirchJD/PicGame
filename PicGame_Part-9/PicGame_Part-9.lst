                                           ;/* Wait for all keys to be released and then return from subroutine.  */
                                           ;/* Log the keys pressed during subroutine, and from before called.    */
                                           ;/* Play a note associated with the keys being pressed during routine. */
                                           ;/**********************************************************************/
0000a8   0830     movf	0x30, w             WAIT_KEY_UP       MOVFW    KEY_PRESS            ; Log current keys being pressed.
0000a9   04b1     iorwf	0x31, f                              IORWF    KEY_LOG
0000aa   01b0     clrf	0x30                                  CLRF     KEY_PRESS            ; Clear the key press flags to check for all keys released.
0000ab   08b3     movf	0x33, f                               MOVF     SIMULTANEOUS, F      ; Play key beep here if only one simultainious LED.
0000ac   1903     btfsc	0x3, 0x2                             BTFSC    STATUS, Z
0000ad   28b0     goto	0xb0                                  GOTO     KEY_UP_BEEP
0000ae   0822     movf	0x22, w                               MOVFW    TEMP1                ; Play key beep appropreate for many simultainious LEDs.
0000af   28b7     goto	0xb7                                  GOTO     KEY_UP_NO_BEEP
0000b0   3000     movlw	0                  KEY_UP_BEEP       MOVLW    0x00                 ; Beep made from merging the two bit key values together.
0000b1   1931     btfsc	0x31, 0x2                            BTFSC    KEY_LOG, SW_LED_2
0000b2   3801     iorlw	0x1                                  IORLW    0x01
0000b3   1831     btfsc	0x31, 0                              BTFSC    KEY_LOG, SW_LED_3
0000b4   3802     iorlw	0x2                                  IORLW    0x02
0000b5   1a31     btfsc	0x31, 0x4                            BTFSC    KEY_LOG, SW_LED_4
0000b6   3803     iorlw	0x3                                  IORLW    0x03
0000b7   3e0a     addlw	0xa                KEY_UP_NO_BEEP    ADDLW    0x0A                 ; Make beep a higher frequency.
0000b8   3810     iorlw	0x10                                 IORLW    0x10                 ; Set beep period to short.
0000b9   20db     call	0xdb                                  CALL     START_BEEP           ; Start playing beep.
0000ba   08a8     movf	0x28, f                               MOVF     PLAYER_TIMEOUT_COUNT, F ; Check for player time out, exit subrouting on time out.
0000bb   1903     btfsc	0x3, 0x2                             BTFSC    STATUS, Z
0000bc   28c1     goto	0xc1                                  GOTO     WAIT_KEYU_END
0000bd   208f     call	0x8f                                  CALL     READ_KEYS            ; Force read of the current keys from the GPIO port.
0000be   08b0     movf	0x30, f                               MOVF     KEY_PRESS, F         ; Check for no keys being pressed.
0000bf   1d03     btfss	0x3, 0x2                             BTFSS    STATUS, Z            ; Exit subrouting when no keys pressed.
0000c0   28a8     goto	0xa8                                  GOTO     WAIT_KEY_UP          ; Loop until no keys are pressed.
0000c1   0008     return                   WAIT_KEYU_END     RETURN
                                           
                                           
                                           
                                           ;/***************************************************************************/
                                           ;/* Wait for no keys being pressed. A very basic version of the subroutine. */
                                           ;/* To make sure no accidental key presses are used when they shouldn't be. */
                                           ;/* GPIO port must be placed into input mode before calling subroutine.     */
                                           ;/* A call to READ_KEYS will put the GPIO ports into input mode.            */
                                           ;/***************************************************************************/
0000c2   0805     movf	0x5, w              WAIT_NO_KEYS      MOVFW    GPIO                 ; Read the GPIO port directly.
0000c3   3aff     xorlw	0xff                                 XORLW    0xFF                 ; Invert the GPIO port values.
0000c4   3935     andlw	0x35                                 ANDLW    (1 << SW_LED_1)|(1 << SW_LED_2)|(1 << SW_LED_3)|(1 << SW_LED_4)
0000c5   1d03     btfss	0x3, 0x2                             BTFSS    STATUS, Z            ; Test only switch related pins.
0000c6   28c2     goto	0xc2                                  GOTO     WAIT_NO_KEYS         ; Loop until no keys pressed.
0000c7   01b0     clrf	0x30                                  CLRF     KEY_PRESS            ; Clear any key press flags to ensure they are not accidentally used.
0000c8   0008     return                                     RETURN
                                           
                                           
                                           
                                           ;/**********************************************************************/
                                           ;/* Pause application. The application still runs operations, but key  */
                                           ;/* presses and pause flagged LED animations are paused.               */
                                           ;/* Allows application to show user specific LED patterns for a period */
                                           ;/* of time.                                                           */
                                           ;/**********************************************************************/
0000c9   00a6     movwf	0x26               PAUSE             MOVWF    PAUSE_COUNT
0000ca   14ae     bsf	0x2e, 0x1                              BSF      APP_FLAGS, F_PAUSED
0000cb   18ae     btfsc	0x2e, 0x1          PAUSE_WAIT        BTFSC    APP_FLAGS, F_PAUSED
0000cc   28cb     goto	0xcb                                  GOTO     PAUSE_WAIT
0000cd   0008     return                                     RETURN
                                           
                                           
                                           
                                           ;/**********************************************************************/
                                           ;/* Whan a tune is being played, an interupt will call this routing to */
                                           ;/* start the next beep playing when the previous beep ends. Or stop   */
                                           ;/* playing at the end of the tune.                                    */
                                           ;/**********************************************************************/
0000ce   082d     movf	0x2d, w             PLAY_NEXT_BEEP    MOVFW    TUNE_PTR             ; Is tune currently playing?
0000cf   1903     btfsc	0x3, 0x2                             BTFSC    STATUS, Z
0000d0   28da     goto	0xda                                  GOTO     NEXT_BEEP_END
0000d1   19ae     btfsc	0x2e, 0x3                            BTFSC    APP_FLAGS, F_SOUND_ACTIVE ; Is beep currently playing?
0000d2   28da     goto	0xda                                  GOTO     NEXT_BEEP_END
0000d3   2126     call	0x126                                 CALL     READ_EEPROM          ; Get the next note to play.
0000d4   1d03     btfss	0x3, 0x2                             BTFSS    STATUS, Z            ; Is note end of tune marker?
0000d5   28d8     goto	0xd8                                  GOTO     PLAY_NEXT_NOTE
0000d6   01ad     clrf	0x2d                                  CLRF     TUNE_PTR             ; Switch off tune at end of tune.
0000d7   28da     goto	0xda                                  GOTO     NEXT_BEEP_END
0000d8   20db     call	0xdb                PLAY_NEXT_NOTE    CALL     START_BEEP           ; Start playing next beep.
0000d9   0aad     incf	0x2d, f                               INCF     TUNE_PTR             ; Point to next note to play.
0000da   0008     return                   NEXT_BEEP_END     RETURN
                                           
                                           
                                           
                                           ;/*************************************************************************/
                                           ;/* Start playing a beep note for a given duration.                       */
                                           ;/*                                                                       */
                                           ;/* CALL WITH:                                                            */
                                           ;/* W - High nibble - Duration of beep.                                   */
                                           ;/* W - Low nibble - Offset to beep frequency in frequency look up table. */
                                           ;/*************************************************************************/
0000db   1d2e     btfss	0x2e, 0x2          START_BEEP        BTFSS    APP_FLAGS, F_SOUND_ON ; Check if sound is enabled.
0000dc   28ee     goto	0xee                                  GOTO     NO_SOUND
0000dd   19ae     btfsc	0x2e, 0x3                            BTFSC    APP_FLAGS, F_SOUND_ACTIVE ; Is beep currently playing?
0000de   28ee     goto	0xee                                  GOTO     NO_SOUND
0000df   00ab     movwf	0x2b                                 MOVWF    BEEP_LEN_L           ; Use W low nibble as the frequency of the beep.
0000e0   00ac     movwf	0x2c                                 MOVWF    BEEP_LEN_H           ; Use W high nibble as the length of the beep.
0000e1   0eac     swapf	0x2c, f                              SWAPF    BEEP_LEN_H           ; Move high nibble to low nibble, beep length.
0000e2   300f     movlw	0xf                                  MOVLW    0x0F                 ; Mask for lower nibble only.
0000e3   05ac     andwf	0x2c, f                              ANDWF    BEEP_LEN_H, F        ; Beep length is only lower nibble.
0000e4   05ab     andwf	0x2b, f                              ANDWF    BEEP_LEN_L, F        ; Use lower nibble of W to lookup frequencey of beep.
0000e5   3000     movlw	0                                    MOVLW    FREQ_LOOKUP
0000e6   072b     addwf	0x2b, w                              ADDWF    BEEP_LEN_L, W        ; Calculate lookup EEPROM address.
0000e7   2126     call	0x126                                 CALL     READ_EEPROM          ; Get beep frequency period for timer 1.
0000e8   00aa     movwf	0x2a                                 MOVWF    BEEP_FREQ_H          ; Set timer periods for beep.
0000e9   008f     movwf	0xf                                  MOVWF    TMR1H
0000ea   01a9     clrf	0x29                                  CLRF     BEEP_FREQ_L
0000eb   018e     clrf	0xe                                   CLRF     TMR1L
0000ec   01ab     clrf	0x2b                                  CLRF     BEEP_LEN_L
0000ed   15ae     bsf	0x2e, 0x3                              BSF      APP_FLAGS, F_SOUND_ACTIVE ; Start beep by switching on timer 1.
0000ee   0008     return                   NO_SOUND          RETURN
                                           
                                           
                                           
                                           ;/***************************************************************************/
                                           ;/* Generate a new random value and place in the next random value RAM      */
                                           ;/* location. Call when the player presses any key to allow timer values to */
                                           ;/* be random.                                                              */
                                           ;/***************************************************************************/
0000ef   3050     movlw	0x50               MAKE_RAND         MOVLW    RAND                 ; Get the address for random values.
0000f0   0739     addwf	0x39, w                              ADDWF    RAND_MAKE_COUNT, W   ; Add the offset to the position to add new values.
0000f1   0084     movwf	0x4                                  MOVWF    FSR                  ; Use indirect addressing to access the data.
0000f2   0e80     swapf	0, f                                 SWAPF    INDF                 ; Shuffle the previous random value.
0000f3   0801     movf	0x1, w                                MOVFW    TMR0                 ; Use the timer 0 value as the source for a new random value.
0000f4   060e     xorwf	0xe, w                               XORWF    TMR1L, W             ; Use logical exclusive or to add timer 1 values.
0000f5   060f     xorwf	0xf, w                               XORWF    TMR1H, W
0000f6   0680     xorwf	0, f                                 XORWF    INDF, F              ; Use logical exclusive or to randomize the last RAM value.
0000f7   0ab9     incf	0x39, f                               INCF     RAND_MAKE_COUNT      ; Point to the next address.
0000f8   300f     movlw	0xf                                  MOVLW    MAX_RAND_COUNT       ; Check if the maximum address for data has been reached. 
0000f9   0639     xorwf	0x39, w                              XORWF    RAND_MAKE_COUNT, W
0000fa   1903     btfsc	0x3, 0x2                             BTFSC    STATUS, Z
0000fb   01b9     clrf	0x39                                  CLRF     RAND_MAKE_COUNT      ; Point back to the start of data when the max address is reached.
0000fc   0008     return                   NO_MAKE_RAND      RETURN
                                           
                                           
                                           
                                           ;/**********************************************************/
                                           ;/* Get the next random value from the random value cache. */
                                           ;/*                                                        */
                                           ;/* RETURNS:                                               */
                                           ;/* W - Random value.                                      */
                                           ;/**********************************************************/
0000fd   0ab8     incf	0x38, f             GET_RAND          INCF     RAND_GET_COUNT       ; Point to the next random value.
0000fe   300f     movlw	0xf                                  MOVLW    MAX_RAND_COUNT       ; Check if the maximum address for data has been reached. 
0000ff   0638     xorwf	0x38, w                              XORWF    RAND_GET_COUNT, W
000100   1903     btfsc	0x3, 0x2                             BTFSC    STATUS, Z
000101   01b8     clrf	0x38                                  CLRF     RAND_GET_COUNT       ; Point back to the start of data when the max address is reached.
000102   3050     movlw	0x50                                 MOVLW    RAND                 ; Get the address for random values.
000103   0738     addwf	0x38, w                              ADDWF    RAND_GET_COUNT, W    ; Add the offset to the position to get next value.
000104   0084     movwf	0x4                                  MOVWF    FSR                  ; Use indirect addressing to access the data.
000105   0800     movf	0, w                                  MOVFW    INDF                 ; Get the next random value.
000106   0008     return                                     RETURN
                                           
                                           
                                           
                                           ;/****************************************************************************/
                                           ;/* Update the LED display, at the GPIO pins of the device. The output of    */
                                           ;/* the actual GPIO outputs is always 0 for all pins. The LEDs are switched  */
                                           ;/* by changing the port direction of each pin. This is done for power       */
                                           ;/* saving reasons. The LEDs are held constantly high on one side, the other */
                                           ;/* side is pulled low by making the pin an output to switch an LED on.      */
                                           ;/* The LEDs are switched off by making the pin an input (high impedence).   */
                                           ;/* This saves power, as when the LED is on - pin pulled low. Pressing a     */
                                           ;/* switch has little effect as this also switches the LED on. But when the  */
                                           ;/* LED is off, the pin is high impedence, so pressing a swtich just         */
                                           ;/* switches the LED on. If the GPIO pin where held high to switch off the   */
                                           ;/* LED, pressing a swtich would not only switch on the LED, but also pull   */
                                           ;/* current from the output of the GPIO pin, and waste battery power.        */
                                           ;/*                                                                          */
                                           ;/* CALL WITH:                                                               */
                                           ;/* LED_STATE - The state of the TRISIO register.                            */
                                           ;/****************************************************************************/
000107   082f     movf	0x2f, w             LED_DISPLAY       MOVFW    LED_STATE            ; Get the current LED state to display.
000108   3aff     xorlw	0xff                                 XORLW    0xFF                 ; Invert the state so TRISIO method lights LEDs correctly.
000109   3935     andlw	0x35                                 ANDLW    (1 << SW_LED_1)|(1 << SW_LED_2)|(1 << SW_LED_3)|(1 << SW_LED_4)
00010a   1683     bsf	0x3, 0x5                               BSF      STATUS, RP0          ; Select Register bank 1
00010b   0085     movwf	0x5                                  MOVWF    TRISIO
00010c   1283     bcf	0x3, 0x5                               BCF      STATUS, RP0          ; Select Register bank 0
00010d   30ca     movlw	0xca                                 MOVLW    ~((1 << SW_LED_1)|(1 << SW_LED_2)|(1 << SW_LED_3)|(1 << SW_LED_4))
00010e   0585     andwf	0x5, f                               ANDWF    GPIO, F              ; Ensure GPIO register is clear for all LEDs.
00010f   0008     return                                     RETURN
                                           
                                           
                                           
                                           ;/***************************************/
                                           ;/* Start a new LED animation sequence. */
                                           ;/*                                     */
                                           ;/* CALL WITH:                          */
                                           ;/* W - Address of new LED animation.   */
                                           ;/***************************************/
000110   01b7     clrf	0x37                SET_LED_ANIM      CLRF     LED_SEQ_COUNT        ; Initialise the LED animation sequence.
000111   00b6     movwf	0x36                                 MOVWF    LED_SEQ_ADR
000112   01af     clrf	0x2f                                  CLRF     LED_STATE            ; Don't light any LEDs at start of LED animation.
000113   2107     call	0x107                                 CALL     LED_DISPLAY          ; Update LEDs.
000114   0008     return                                     RETURN
                                           
                                           
                                           
                                           ;/*******************************************/
                                           ;/* Animate to next LED animation sequence. */
                                           ;/*******************************************/
000115   1cae     btfss	0x2e, 0x1          NEXT_LED_ANIM_SEQ BTFSS    APP_FLAGS, F_PAUSED  ; Check if application is paused.
000116   2919     goto	0x119                                 GOTO     SEQ_NO_PAUSE
000117   1baf     btfsc	0x2f, 0x7                            BTFSC    LED_STATE, F_LED_SEQ_PAUSE ; Don't change LED sequence if flagged to pause with application.
000118   2925     goto	0x125                                 GOTO     END_EEPROM_SEQ
000119   0836     movf	0x36, w             SEQ_NO_PAUSE      MOVFW    LED_SEQ_ADR          ; Get the address for the current animation sequence.
00011a   0737     addwf	0x37, w                              ADDWF    LED_SEQ_COUNT, W     ; Add the offset to the position to get next value.
00011b   0ab7     incf	0x37, f                               INCF     LED_SEQ_COUNT        ; Point to the next address.
00011c   2126     call	0x126                                 CALL     READ_EEPROM          ; Read the next animation value.
00011d   3aff     xorlw	0xff                                 XORLW    0xFF                 ; Check for end of animation.
00011e   1d03     btfss	0x3, 0x2                             BTFSS    STATUS, Z            ; A value of 0xFF, is the end of sequence.
00011f   2923     goto	0x123                                 GOTO     SET_EEPROM_SEQ
000120   01b7     clrf	0x37                                  CLRF     LED_SEQ_COUNT        ; Point to the start of sequence when at end.
000121   2115     call	0x115                                 CALL     NEXT_LED_ANIM_SEQ    ; Get the start animation value, by recursion.
000122   3aff     xorlw	0xff                                 XORLW    0xFF                 ; Adjust for recursion.
000123   3aff     xorlw	0xff               SET_EEPROM_SEQ    XORLW    0xFF                 ; Undo end of sequence check effect on W.
000124   00af     movwf	0x2f                                 MOVWF    LED_STATE            ; Update LEDs.
000125   0008     return                   END_EEPROM_SEQ    RETURN
                                           
                                           
                                           
                                           ;/****************************************/
                                           ;/* Read a value from an EEPROM address. */
                                           ;/*                                      */
                                           ;/* CALL WITH:                           */
                                           ;/* W - EEPROM address.                  */
                                           ;/*                                      */
                                           ;/* RETURNS WITH:                        */
                                           ;/* W - EEPROM value read.               */
                                           ;/****************************************/
000126   1683     bsf	0x3, 0x5             READ_EEPROM       BSF      STATUS, RP0          ; Select Register bank 1
000127   009b     movwf	0x1b                                 MOVWF    EEADR                ; Point to the address in EEPROM.
000128   141c     bsf	0x1c, 0                                BSF      EECON1, RD           ; Read from EEPROM.
000129   081a     movf	0x1a, w                               MOVFW    EEDATA               ; Read the value from EEPROM.
00012a   1283     bcf	0x3, 0x5                               BCF      STATUS, RP0          ; Select Register bank 0
00012b   0008     return                                     RETURN
                                           
                                           
                                           
                                           ;/*****************************/
                                           ;/* Reset a RAM area to 0x00. */
                                           ;/*                           */
                                           ;/* CALL WITH:                */
                                           ;/* FSR - Start RAM address.  */
                                           ;/* W   - Byte clear count.   */
                                           ;/*****************************/
00012c   0180     clrf	0                   CLEAR_RAM         CLRF     INDF                 ; Clear RAM address.
00012d   0a84     incf	0x4, f                                INCF     FSR                  ; Point to next RAM address,
00012e   3eff     addlw	0xff                                 ADDLW    0xFF                 ; Subtract 1 from loop count.
00012f   1d03     btfss	0x3, 0x2                             BTFSS    STATUS, Z
000130   292c     goto	0x12c                                 GOTO     CLEAR_RAM            ; Loop until all RAM addresses cleared.
000131   0008     return                                     RETURN
                                           
                                           
                                           
