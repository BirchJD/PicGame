                                           ;/* Allows application to show user specific LED patterns for a period */
                                           ;/* of time.                                                           */
                                           ;/**********************************************************************/
0000ae   00a6     movwf	0x26               PAUSE             MOVWF    PAUSE_COUNT
0000af   14ae     bsf	0x2e, 0x1                              BSF      APP_FLAGS, F_PAUSED
0000b0   18ae     btfsc	0x2e, 0x1          PAUSE_WAIT        BTFSC    APP_FLAGS, F_PAUSED
0000b1   28b0     goto	0xb0                                  GOTO     PAUSE_WAIT
0000b2   0008     return                                     RETURN
                                           
                                           
                                           
                                           ;/**********************************************************************/
                                           ;/* Whan a tune is being played, an interupt will call this routing to */
                                           ;/* start the next beep playing when the previous beep ends. Or stop   */
                                           ;/* playing at the end of the tune.                                    */
                                           ;/**********************************************************************/
0000b3   082d     movf	0x2d, w             PLAY_NEXT_BEEP    MOVFW    TUNE_PTR             ; Is tune currently playing?
0000b4   1903     btfsc	0x3, 0x2                             BTFSC    STATUS, Z
0000b5   28bf     goto	0xbf                                  GOTO     NEXT_BEEP_END
0000b6   19ae     btfsc	0x2e, 0x3                            BTFSC    APP_FLAGS, F_SOUND_ACTIVE ; Is beep currently playing?
0000b7   28bf     goto	0xbf                                  GOTO     NEXT_BEEP_END
0000b8   20f3     call	0xf3                                  CALL     READ_EEPROM          ; Get the next note to play.
0000b9   1d03     btfss	0x3, 0x2                             BTFSS    STATUS, Z            ; Is note end of tune marker?
0000ba   28bd     goto	0xbd                                  GOTO     PLAY_NEXT_NOTE
0000bb   01ad     clrf	0x2d                                  CLRF     TUNE_PTR             ; Switch off tune at end of tune.
0000bc   28bf     goto	0xbf                                  GOTO     NEXT_BEEP_END
0000bd   20c0     call	0xc0                PLAY_NEXT_NOTE    CALL     START_BEEP           ; Start playing next beep.
0000be   0aad     incf	0x2d, f                               INCF     TUNE_PTR             ; Point to next note to play.
0000bf   0008     return                   NEXT_BEEP_END     RETURN
                                           
                                           
                                           
                                           ;/*************************************************************************/
                                           ;/* Start playing a beep note for a given duration.                       */
                                           ;/*                                                                       */
                                           ;/* CALL WITH:                                                            */
                                           ;/* W - High nibble - Duration of beep.                                   */
                                           ;/* W - Low nibble - Offset to beep frequency in frequency look up table. */
                                           ;/*************************************************************************/
0000c0   1d2e     btfss	0x2e, 0x2          START_BEEP        BTFSS    APP_FLAGS, F_SOUND_ON ; Check if sound is enabled.
0000c1   28d3     goto	0xd3                                  GOTO     NO_SOUND
0000c2   19ae     btfsc	0x2e, 0x3                            BTFSC    APP_FLAGS, F_SOUND_ACTIVE ; Is beep currently playing?
0000c3   28d3     goto	0xd3                                  GOTO     NO_SOUND
0000c4   00ab     movwf	0x2b                                 MOVWF    BEEP_LEN_L           ; Use W low nibble as the frequency of the beep.
0000c5   00ac     movwf	0x2c                                 MOVWF    BEEP_LEN_H           ; Use W high nibble as the length of the beep.
0000c6   0eac     swapf	0x2c, f                              SWAPF    BEEP_LEN_H           ; Move high nibble to low nibble, beep length.
0000c7   300f     movlw	0xf                                  MOVLW    0x0F                 ; Mask for lower nibble only.
0000c8   05ac     andwf	0x2c, f                              ANDWF    BEEP_LEN_H, F        ; Beep length is only lower nibble.
0000c9   05ab     andwf	0x2b, f                              ANDWF    BEEP_LEN_L, F        ; Use lower nibble of W to lookup frequencey of beep.
0000ca   3000     movlw	0                                    MOVLW    FREQ_LOOKUP
0000cb   072b     addwf	0x2b, w                              ADDWF    BEEP_LEN_L, W        ; Calculate lookup EEPROM address.
0000cc   20f3     call	0xf3                                  CALL     READ_EEPROM          ; Get beep frequency period for timer 1.
0000cd   00aa     movwf	0x2a                                 MOVWF    BEEP_FREQ_H          ; Set timer periods for beep.
0000ce   008f     movwf	0xf                                  MOVWF    TMR1H
0000cf   01a9     clrf	0x29                                  CLRF     BEEP_FREQ_L
0000d0   018e     clrf	0xe                                   CLRF     TMR1L
0000d1   01ab     clrf	0x2b                                  CLRF     BEEP_LEN_L
0000d2   15ae     bsf	0x2e, 0x3                              BSF      APP_FLAGS, F_SOUND_ACTIVE ; Start beep by switching on timer 1.
0000d3   0008     return                   NO_SOUND          RETURN
                                           
                                           
                                           
                                           ;/****************************************************************************/
                                           ;/* Update the LED display, at the GPIO pins of the device. The output of    */
                                           ;/* the actual GPIO outputs is always 0 for all pins. The LEDs are switched  */
                                           ;/* by changing the port direction of each pin. This is done for power       */
                                           ;/* saving reasons. The LEDs are held constantly high on one side, the other */
                                           ;/* side is pulled low by making the pin an output to switch an LED on.      */
                                           ;/* The LEDs are switched off by making the pin an input (high impedence).   */
                                           ;/* This saves power, as when the LED is on - pin pulled low. Pressing a     */
                                           ;/* switch has little effect as this also switches the LED on. But when the  */
                                           ;/* LED is off, the pin is high impedence, so pressing a swtich just         */
                                           ;/* switches the LED on. If the GPIO pin where held high to switch off the   */
                                           ;/* LED, pressing a swtich would not only switch on the LED, but also pull   */
                                           ;/* current from the output of the GPIO pin, and waste battery power.        */
                                           ;/*                                                                          */
                                           ;/* CALL WITH:                                                               */
                                           ;/* LED_STATE - The state of the TRISIO register.                            */
                                           ;/****************************************************************************/
0000d4   082f     movf	0x2f, w             LED_DISPLAY       MOVFW    LED_STATE            ; Get the current LED state to display.
0000d5   3aff     xorlw	0xff                                 XORLW    0xFF                 ; Invert the state so TRISIO method lights LEDs correctly.
0000d6   3935     andlw	0x35                                 ANDLW    (1 << SW_LED_1)|(1 << SW_LED_2)|(1 << SW_LED_3)|(1 << SW_LED_4)
0000d7   1683     bsf	0x3, 0x5                               BSF      STATUS, RP0          ; Select Register bank 1
0000d8   0085     movwf	0x5                                  MOVWF    TRISIO
0000d9   1283     bcf	0x3, 0x5                               BCF      STATUS, RP0          ; Select Register bank 0
0000da   30ca     movlw	0xca                                 MOVLW    ~((1 << SW_LED_1)|(1 << SW_LED_2)|(1 << SW_LED_3)|(1 << SW_LED_4))
0000db   0585     andwf	0x5, f                               ANDWF    GPIO, F              ; Ensure GPIO register is clear for all LEDs.
0000dc   0008     return                                     RETURN
                                           
                                           
                                           
                                           ;/***************************************/
                                           ;/* Start a new LED animation sequence. */
                                           ;/*                                     */
                                           ;/* CALL WITH:                          */
                                           ;/* W - Address of new LED animation.   */
                                           ;/***************************************/
0000dd   01b5     clrf	0x35                SET_LED_ANIM      CLRF     LED_SEQ_COUNT        ; Initialise the LED animation sequence.
0000de   00b4     movwf	0x34                                 MOVWF    LED_SEQ_ADR
0000df   01af     clrf	0x2f                                  CLRF     LED_STATE            ; Don't light any LEDs at start of LED animation.
0000e0   20d4     call	0xd4                                  CALL     LED_DISPLAY          ; Update LEDs.
0000e1   0008     return                                     RETURN
                                           
                                           
                                           
                                           ;/*******************************************/
                                           ;/* Animate to next LED animation sequence. */
                                           ;/*******************************************/
0000e2   1cae     btfss	0x2e, 0x1          NEXT_LED_ANIM_SEQ BTFSS    APP_FLAGS, F_PAUSED  ; Check if application is paused.
0000e3   28e6     goto	0xe6                                  GOTO     SEQ_NO_PAUSE
0000e4   1baf     btfsc	0x2f, 0x7                            BTFSC    LED_STATE, F_LED_SEQ_PAUSE ; Don't change LED sequence if flagged to pause with application.
0000e5   28f2     goto	0xf2                                  GOTO     END_EEPROM_SEQ
0000e6   0834     movf	0x34, w             SEQ_NO_PAUSE      MOVFW    LED_SEQ_ADR          ; Get the address for the current animation sequence.
0000e7   0735     addwf	0x35, w                              ADDWF    LED_SEQ_COUNT, W     ; Add the offset to the position to get next value.
0000e8   0ab5     incf	0x35, f                               INCF     LED_SEQ_COUNT        ; Point to the next address.
0000e9   20f3     call	0xf3                                  CALL     READ_EEPROM          ; Read the next animation value.
0000ea   3aff     xorlw	0xff                                 XORLW    0xFF                 ; Check for end of animation.
0000eb   1d03     btfss	0x3, 0x2                             BTFSS    STATUS, Z            ; A value of 0xFF, is the end of sequence.
0000ec   28f0     goto	0xf0                                  GOTO     SET_EEPROM_SEQ
0000ed   01b5     clrf	0x35                                  CLRF     LED_SEQ_COUNT        ; Point to the start of sequence when at end.
0000ee   20e2     call	0xe2                                  CALL     NEXT_LED_ANIM_SEQ    ; Get the start animation value, by recursion.
0000ef   3aff     xorlw	0xff                                 XORLW    0xFF                 ; Adjust for recursion.
0000f0   3aff     xorlw	0xff               SET_EEPROM_SEQ    XORLW    0xFF                 ; Undo end of sequence check effect on W.
0000f1   00af     movwf	0x2f                                 MOVWF    LED_STATE            ; Update LEDs.
0000f2   0008     return                   END_EEPROM_SEQ    RETURN
                                           
                                           
                                           
                                           ;/****************************************/
                                           ;/* Read a value from an EEPROM address. */
                                           ;/*                                      */
                                           ;/* CALL WITH:                           */
                                           ;/* W - EEPROM address.                  */
                                           ;/*                                      */
                                           ;/* RETURNS WITH:                        */
                                           ;/* W - EEPROM value read.               */
                                           ;/****************************************/
0000f3   1683     bsf	0x3, 0x5             READ_EEPROM       BSF      STATUS, RP0          ; Select Register bank 1
0000f4   009b     movwf	0x1b                                 MOVWF    EEADR                ; Point to the address in EEPROM.
0000f5   141c     bsf	0x1c, 0                                BSF      EECON1, RD           ; Read from EEPROM.
0000f6   081a     movf	0x1a, w                               MOVFW    EEDATA               ; Read the value from EEPROM.
0000f7   1283     bcf	0x3, 0x5                               BCF      STATUS, RP0          ; Select Register bank 0
0000f8   0008     return                                     RETURN
                                           
                                           
                                           
                                           ;/*****************************/
                                           ;/* Reset a RAM area to 0x00. */
                                           ;/*                           */
                                           ;/* CALL WITH:                */
                                           ;/* FSR - Start RAM address.  */
                                           ;/* W   - Byte clear count.   */
                                           ;/*****************************/
0000f9   0180     clrf	0                   CLEAR_RAM         CLRF     INDF                 ; Clear RAM address.
0000fa   0a84     incf	0x4, f                                INCF     FSR                  ; Point to next RAM address,
0000fb   3eff     addlw	0xff                                 ADDLW    0xFF                 ; Subtract 1 from loop count.
0000fc   1d03     btfss	0x3, 0x2                             BTFSS    STATUS, Z
0000fd   28f9     goto	0xf9                                  GOTO     CLEAR_RAM            ; Loop until all RAM addresses cleared.
0000fe   0008     return                                     RETURN
                                           
                                           
                                           
